# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here
CLIENT_ID=your_discord_application_id_here
CLIENT_SECRET=your_discord_client_secret_here

# Database Choice - Set to "supabase" or "mongodb"
DATABASE_TYPE=supabase

# MongoDB Connection (if using MongoDB)
MONGODB_URI=mongodb://localhost:27017/discord-verification

# Supabase Configuration (if using Supabase)
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# reCAPTCHA Configuration
RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here
RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key_here

# Twilio SMS Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Web Service Configuration
PORT=3000
BASE_URL=http://localhost:3000

# Development Mode (set to true to use mock services)
USE_MOCK_MODE=false